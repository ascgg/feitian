{
  "name": "FeiTian",
  "productName": "飞天",
  "version": "1.0.0",
  "description": "造价表数据库同步软件",
  "private": true,
  "license": "ISC",
  "author": "Canis Technology",
  "main": "src/browser/main.js",
  "devDependencies": {
    "ag-grid": "^6.2.1",
    "ag-grid-react": "^6.2.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "breakpoint-sass": "^2.7.1",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "1.8.3",
    "electron-log": "^2.2.4",
    "electron-packager": "^8.3.0",
    "electron-winstaller": "^2.5.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.0",
    "imports-loader": "^0.6.5",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "mysql": "^2.11.1",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "semantic-ui": "^2.2.4",
    "sequelize": "^3.24.4",
    "signcode": "^0.5.0",
    "spawn-auto-restart": "^2.0.1",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "xlsx": "^0.8.0"
  },
  "scripts": {
    "dev": "concurrently --names 'webpack,electron' --prefix name 'npm run dev:webpack' 'npm run dev:electron'",
    "dev:webpack": "webpack-dev-server --color --config ./webpack.dev.config.js",
    "dev:electron": "cross-env DEBUG=FeiTian:*,spawn-auto-restart node scripts/dev-auto-restart.js",
    "clean": "rimraf out",
    "clean:osx": "rimraf out/飞天-darwin*",
    "clean:win": "rimraf out/飞天-win*",
    "compile": "rimraf out && npm run compile:browser && npm run compile:renderer",
    "compile:browser": "cross-env NODE_ENV=production babel ./src/browser -d build/browser",
    "compile:renderer": "cross-env NODE_ENV=production webpack --progress --display-error-details --config ./webpack.prod.config.js",
    "pack-mac": "npm run compile && electron-packager ./build --asar --overwrite --platform=darwin --arch=x64 --icon=assets/app.icns --prune=true --out=out",
    "pack-win": "npm run compile && electron-packager ./build 飞天 --asar --overwrite --platform=win32 --arch=x64 --icon=assets/app.ico --prune=true --out=out --version-string.CompanyName='Canis Technology, Co., Ltd.' --version-string.FileDescription='造价表同步数据库软件' --version-string.ProductName='飞天'",
    "installer": "node ./scripts/installer.js"
  },
  "dependencies": {}
}
